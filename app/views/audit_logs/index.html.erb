<h2 class="my-3">List of Audit Logs</h2>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Employee</th>
      <th scope="col">Week Starting</th>
      <th scope="col">Confirmed At</th>
      <th scope="col">Status</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody id="audit_logs">
    <%= render @audit_logs %>
  </tbody>
</table>

<div id="paginator">
  <%= paginate @audit_logs, remote: true %>
</div>